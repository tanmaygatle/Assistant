      android:versionName="1.0">
    
     <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
           <!-- android:targetSdkVersion="?" -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_INTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO"></uses-permission>
    
    <application android:label="VoiceRecognize"
        android:icon="@drawable/ic_launcher" 
        android:allowBackup="false">
        
        <activity android:name="com.example.tanmay.spkdet.VoiceRecognize"
                  android:label="VoiceRecognize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.tanmay.spkdet.RecordForTraining"
                  android:label="Please help the App learn your voice.">
            <intent-filter>
                <action android:name="com.example.tanmay.spkdet.RecordForTraining" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        
 
        
